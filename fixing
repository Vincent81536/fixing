local Reanimate = loadstring(game:HttpGet("https://raw.githubusercontent.com/Vincent81536/module-/refs/heads/main/EmperyeanReanimate"))()

Reanimate.Start({
    SetCharacter = true,
    SetCameraSubject = true,
    SetCameraType = true,
    DisableCharacterCollisions = true,
    DisableRigCollisions = true,
    SimulationRadius = math.huge,
    ApplyDescription = true,
    HideCharacter = Vector3.new(0, -30, 0),
    BreakJointsDelay = 0,
    PermanentDeath = true,
    Refit = true,
    RigTransparency = 1,
    HatDrop = false,
    R15 = false -- set to true if your avatar is R15
})
